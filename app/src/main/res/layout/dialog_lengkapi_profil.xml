<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="30dp"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/logo"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/new_logo" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_regular"
        android:text="Lengkapi Data Profil Terlebih Dahulu"
        android:textColor="@color/font_grey"
        android:layout_marginTop="5dp"
        android:textSize="10sp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:layout_weight="2">
        <RelativeLayout
            android:id="@+id/btn_lengkapi_data_profil"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/frame_button_white_top_stroke_gray">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:fontFamily="@font/inter_regular"
                android:text="Lengkapi Data Profil"
                android:textColor="#16672E"
                android:textSize="12sp" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/btn_kembali"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/frame_button_white_top_left_stroke_gray">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_regular"
                android:text="Kembali"
                android:textColor="@color/black"
                android:layout_centerInParent="true"
                android:textSize="12sp"/>
        </RelativeLayout>

    </LinearLayout>

</LinearLayout>